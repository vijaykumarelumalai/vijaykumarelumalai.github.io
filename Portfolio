    <script>
        //common reveal options to create reveal animations
        ScrollReveal({
            reset: true,
            distance: '60px',
            duration: 1600,
            delay: 350
        });
        //target elements, and specify options to create reveal animations
        ScrollReveal().reveal('.main-title, .section-title', {
            delay: 400,
            origin: 'left'
        });
        ScrollReveal().reveal('.sec-01 .image, .info', {
            delay: 400,
            origin: 'bottom'
        });
        ScrollReveal().reveal('.text-box', {
            delay: 500,
            origin: 'right'
        });
        ScrollReveal().reveal('.social-links i', {
            delay: 450,
            origin: 'bottom',
            interval: 200
        });
        ScrollReveal().reveal('.sec-02 .image, .sec-3 .image', {
            delay: 450,
            origin: 'top'
        });
